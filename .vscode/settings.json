{
  "deepscan.enable": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "typescript.tsdk": "absolute_path_to/node_modules/typescript/lib"
  }
  
}
